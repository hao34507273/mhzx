  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300076" taskVoiceId="720090081" templatename="主线配音" voiceDes="普泓/乖徒儿回来了，此次外出历练可有收获？" voiceId="77"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300077" taskVoiceId="720090082" templatename="主线配音" voiceDes="普泓/修行如逆水行舟，不进则退，你能有此认识，为师深感欣慰。" voiceId="78"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300078" taskVoiceId="720090083" templatename="主线配音" voiceDes="普泓/我知道你一心想要彻查草庙村之案，为师在京城有一故人，身居高位，或许能助你一臂之力。" voiceId="79"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300079" taskVoiceId="720090084" templatename="主线配音" voiceDes="普泓/少拍马屁，快去吧，出门在外，也不可荒废修行。" voiceId="80"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300080" taskVoiceId="720090085" templatename="主线配音" voiceDes="原来是故人之徒，果然气质非凡！" voiceId="81"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300081" taskVoiceId="720090086" templatename="主线配音" voiceDes="草庙村惨案震惊朝野，但此事有修真人士介入，已超出朝廷的管辖范围。此事发生在青云山下，多少会和青云门有些干系。" voiceId="82"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300082" taskVoiceId="720095001" templatename="剧情配音" voiceDes="开场文字：方今之世，修仙盛行，正有青云门、天音寺盛名天下，魔有鬼王宗、合欢派独霸一方，更有圣巫教、焚香谷神秘莫测。我们的故事便由此开始......" voiceId="5001"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300083" taskVoiceId="720095002" templatename="剧情配音" voiceDes="灵尊（水麒麟）BOSS：（暴怒嘶吼声）杀杀杀！" voiceId="5002"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300084" taskVoiceId="720095003" templatename="剧情配音" voiceDes="迎客弟子：救命啊！灵尊入魔了！" voiceId="5003"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300085" taskVoiceId="720095004" templatename="剧情配音" voiceDes="陈御风：孽畜！休要伤人！" voiceId="5004"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300086" taskVoiceId="720095005" templatename="剧情配音" voiceDes="灵尊（水麒麟）BOSS：哼！一群蝼蚁，能奈我何！" voiceId="5005"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300087" taskVoiceId="720095006" templatename="剧情配音" voiceDes="七杀：孽畜！敢小瞧我们！" voiceId="5006"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300088" taskVoiceId="720095007" templatename="剧情配音" voiceDes="灵尊（水麒麟）BOSS：哼！你们这点能耐，本尊根本就不放在眼里！" voiceId="5007"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300089" taskVoiceId="720095008" templatename="剧情配音" voiceDes="灵尊（水麒麟）BOSS：看本尊的诛仙剑阵！" voiceId="5008"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300090" taskVoiceId="720095009" templatename="剧情配音" voiceDes="鬼厉：糟糕！灵尊入魔了！" voiceId="5009"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300091" taskVoiceId="720095010" templatename="剧情配音" voiceDes="碧瑶：小凡哥哥，怎么办？" voiceId="5010"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300092" taskVoiceId="720095011" templatename="剧情配音" voiceDes="陆雪琪：我们去阻止它，否则必将生灵涂炭！" voiceId="5011"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300093" taskVoiceId="720095012" templatename="剧情配音" voiceDes="鬼厉：琪儿说的对，我们快去阻止他！" voiceId="5012"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300094" taskVoiceId="720095013" templatename="剧情配音" voiceDes="灵尊（水麒麟）BOSS：（死亡前嘶吼声）我不甘心！（死亡）" voiceId="5013"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300095" taskVoiceId="720095014" templatename="剧情配音" voiceDes="鬼厉：阁下好身手，竟有能力降服入魔的灵尊！" voiceId="5014"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300096" taskVoiceId="720095015" templatename="剧情配音" voiceDes="小白：咦！那是什么？" voiceId="5015"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300097" taskVoiceId="720095016" templatename="剧情配音" voiceDes="碧瑶：小凡哥哥！快看！" voiceId="5016"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300098" taskVoiceId="720095017" templatename="剧情配音" voiceDes="陆雪琪：大劫过后竟有两只灵宠幸存！" voiceId="5017"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300099" taskVoiceId="720095018" templatename="剧情配音" voiceDes="菜刀兔：是谁打败了大魔头？本宠愿与你共闯天下！" voiceId="5018"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300100" taskVoiceId="720095019" templatename="剧情配音" voiceDes="爱心兔：太好啦，终于安全了！" voiceId="5019"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300101" taskVoiceId="720095020" templatename="剧情配音" voiceDes="林惊羽：救命呀！救命呀！" voiceId="5020"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300102" taskVoiceId="720095021" templatename="剧情配音" voiceDes="神秘人：站住！" voiceId="5021"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300104" taskVoiceId="720095023" templatename="剧情配音" voiceDes="普智：快快住手！" voiceId="5023"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300105" taskVoiceId="720095024" templatename="剧情配音" voiceDes="神秘人：哪来的秃驴！竟敢多管闲事！" voiceId="5024"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300106" taskVoiceId="720090087" templatename="剧情配音" voiceDes="来者何人，青云门虽不拒来客，只是近日魔教探子猖獗，还望阁下通报大名。" voiceId="5025"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300107" taskVoiceId="720090088" templatename="剧情配音" voiceDes="原来如此，说来此事真让人痛心，既然那两孩子与你有渊源，那么便上山吧。" voiceId="5026"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300108" taskVoiceId="720090089" templatename="剧情配音" voiceDes="不过山门内多有巡逻弟子，有些师兄弟性子急，你可莫要与他们动粗。" voiceId="5027"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300109" taskVoiceId="720090090" templatename="剧情配音" voiceDes="想不到这么多人都打不过你……好，算你厉害……杀了我们吧！我们既然无力保卫师门，也只能以死殉道了……动手吧！" voiceId="5028"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300110" taskVoiceId="720090091" templatename="剧情配音" voiceDes="……真的？哎，都怪我一时糊涂啊……" voiceId="5029"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300111" taskVoiceId="720090092" templatename="剧情配音" voiceDes="唉，掌门得知草庙村惨案后又惊又怒，此刻七脉首座都在玉清殿内，商量该如何安置两个遗孤呢。" voiceId="5030"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300112" taskVoiceId="720090093" templatename="剧情配音" voiceDes="你没事吧，灵尊没有伤到你吧。" voiceId="5031"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300113" taskVoiceId="720090094" templatename="剧情配音" voiceDes="阁下有所不知，灵尊乃青云门守山灵兽。若非有熟悉灵尊的弟子结伴，生人是无法通过的。" voiceId="5032"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300114" taskVoiceId="720090095" templatename="剧情配音" voiceDes="是我们青云门疏忽了，阁下不要介意，如今掌门和各位首座都在殿内商议，阁下不妨前往一观。" voiceId="5033"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300115" taskVoiceId="720090096" templatename="剧情配音" voiceDes="鬼！恶鬼！鬼啊！……" voiceId="5034"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300116" taskVoiceId="720090097" templatename="剧情配音" voiceDes="好了，此案就此告一段落，不过依然疑点重重，诸位师弟要继续留心调查，明白了么？" voiceId="5035"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300117" taskVoiceId="720090098" templatename="剧情配音" voiceDes="阁下为何一路跟随？" voiceId="5036"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300118" taskVoiceId="720090099" templatename="剧情配音" voiceDes="原来是保护小师弟的那位义士！你是我青云门贵客，只是这山居清苦，小师弟年纪又小，还要麻烦阁下多关照。" voiceId="5037"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300119" taskVoiceId="720090100" templatename="剧情配音" voiceDes="快醒醒，是时候去做功课了。" voiceId="5038"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300120" taskVoiceId="720090101" templatename="剧情配音" voiceDes="大师兄说了，这是入门的基础，每个新弟子都要用心才行。" voiceId="5039"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300121" taskVoiceId="720090102" templatename="剧情配音" voiceDes="好累呀，终于做完了功课，不知师姐在干什么，你陪我去找师姐吧。" voiceId="5040"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300122" taskVoiceId="720090103" templatename="剧情配音" voiceDes="原来是小凡呀，你们是来观摩我做功课的吗？" voiceId="5041"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300123" taskVoiceId="720090104" templatename="剧情配音" voiceDes="田灵儿/咱们青云山，钟灵毓秀，正是山灵精怪喜好之地。若它们安分守己也罢，却偏偏时常侵扰山门，因此我每日都要过来清理一番。" voiceId="5042"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300124" taskVoiceId="720090105" templatename="剧情配音" voiceDes="张小凡/师姐！你好厉害呀！" voiceId="5043"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300125" taskVoiceId="720090106" templatename="剧情配音" voiceDes="田灵儿/气死我了，好滑溜的猴子，不但欺负小凡，连我也敢挑衅！我一定要将其捉住，狠狠教训一番。" voiceId="5044"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300126" taskVoiceId="720090107" templatename="剧情配音" voiceDes="田灵儿/好呀，好呀，我这里有个荷包，里面是娘亲给我做的百香蜜饯，用它来引诱这只猴子，肯定能成。" voiceId="5045"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300127" taskVoiceId="720090108" templatename="剧情配音" voiceDes="田灵儿/哈哈，这猴子果然上当了！" voiceId="5046"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300128" taskVoiceId="720090109" templatename="剧情配音" voiceDes="田灵儿/哎呀，这死猴子，别跑！" voiceId="5047"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300129" taskVoiceId="720090110" templatename="剧情配音" voiceDes="张小凡/师姐你慢点，等等我们！" voiceId="5048"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300130" taskVoiceId="720090111" templatename="剧情配音" voiceDes="田灵儿/咦，这里是什么地方，快看，那只猴子似乎被定在前面的水潭旁了，我们快去抓住它！" voiceId="5049"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300131" taskVoiceId="720090112" templatename="剧情配音" voiceDes="田灵儿/（田灵儿冒冒失失地走近水潭）呃……头好晕，好难受……" voiceId="5050"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300132" taskVoiceId="720090113" templatename="剧情配音" voiceDes="张小凡/你们怎么了？没事吧？啊，师姐晕倒了！" voiceId="5051"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300133" taskVoiceId="720090114" templatename="剧情配音" voiceDes="张小凡/我？或许是这串佛珠护身的缘故吧。" voiceId="5052"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300134" taskVoiceId="720090115" templatename="剧情配音" voiceDes="张小凡/咦，这串佛珠里面有颗好奇怪，似乎有一股青气在往外冲，啊，佛珠外面的佛偈碎了！" voiceId="5053"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300135" taskVoiceId="720090116" templatename="剧情配音" voiceDes="张小凡/（恰在此时，自水潭中飞出一根诡异的短棍，冲向张小凡手中的珠子）它……它们似乎在吸我的血，好难受……" voiceId="5054"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300136" taskVoiceId="720090117" templatename="剧情配音" voiceDes="田灵儿/刚才到底发生了什么？我怎么晕了过去？啊，小凡怎么了，为何脸色如此苍白！" voiceId="5055"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300137" taskVoiceId="720090118" templatename="剧情配音" voiceDes="田灵儿/是我太冲动了，连累了小凡…对了，这根奇怪的棍子是什么？还有那只猴子怎么不跑了？" voiceId="5056"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300138" taskVoiceId="720090119" templatename="剧情配音" voiceDes="张小凡/极是极是，我们快离开这里！" voiceId="5057"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300139" taskVoiceId="720090120" templatename="剧情配音" voiceDes="宋大仁/什么！小师弟遇险失血过多？这么大的事情还不得赶紧禀告师父和师娘。" voiceId="5058"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300140" taskVoiceId="720090121" templatename="剧情配音" voiceDes="宋大仁/对对，补血的药材…对了！我知道南疆生长着一种奇特的血脂草，这种药草对失血过多的人有奇效。" voiceId="5059"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300141" taskVoiceId="720090122" templatename="剧情配音" voiceDes="张小凡/多谢你，我……我……" voiceId="5060"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300142" taskVoiceId="720090123" templatename="剧情配音" voiceDes="张小凡/我很久没有向大师兄汇报修行进展了，你陪我一起去吧。" voiceId="5061"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300143" taskVoiceId="720090124" templatename="剧情配音" voiceDes="宋大仁/小师弟最近进步神速，说不定可以在七脉会武上一展身手呢。" voiceId="5062"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300144" taskVoiceId="720090125" templatename="剧情配音" voiceDes="宋大仁/七脉会武乃青云门一甲子一次的盛会，意在提携一些修行有成的年轻弟子，说来距离这次会武也只有两年时间了。" voiceId="5063"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300145" taskVoiceId="720090126" templatename="剧情配音" voiceDes="宋大仁/说来惭愧呀，宋某最后败在了龙首峰门下。" voiceId="5064"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300146" taskVoiceId="720090127" templatename="剧情配音" voiceDes="齐昊/龙首峰苍松道人座下弟子齐昊、林惊羽，拜见田师叔、苏师叔。" voiceId="5065"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300147" taskVoiceId="720090128" templatename="剧情配音" voiceDes="田不易/你师父让你们来做什么？" voiceId="5066"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300148" taskVoiceId="720090129" templatename="剧情配音" voiceDes="齐昊/禀田师叔，家师受掌门真人所托，着手打理两年后“七脉会武”事宜，因为有少许变动，故特命我与林师弟一同前来通报。" voiceId="5067"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300149" taskVoiceId="720090130" templatename="剧情配音" voiceDes="田不易/怎么回事？" voiceId="5068"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300150" taskVoiceId="720090131" templatename="剧情配音" voiceDes="齐昊/家师认为近年青云门广纳人才，若还以每脉派出四名弟子出战，为免有沧海遗珠之憾，所以决定七脉各出弟子九人……" voiceId="5069"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300151" taskVoiceId="720090132" templatename="剧情配音" voiceDes="林惊羽/小凡，趁着田师叔与齐师兄正商议会武事宜，我们去外头叙叙旧。" voiceId="5070"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300152" taskVoiceId="720090133" templatename="剧情配音" voiceDes="张小凡/嗯，我有好多话想和你说。" voiceId="5071"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300153" taskVoiceId="720090134" templatename="剧情配音" voiceDes="林惊羽/你长高了，小凡。" voiceId="5072"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300154" taskVoiceId="720090135" templatename="剧情配音" voiceDes="张小凡/你也是，对了，村子的事你有没有什么新的消息？" voiceId="5073"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300155" taskVoiceId="720090136" templatename="剧情配音" voiceDes="林惊羽/唉，还是没有什么进展。" voiceId="5074"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300156" taskVoiceId="720090137" templatename="剧情配音" voiceDes="张小凡/……没想到还没有线索。" voiceId="5075"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300157" taskVoiceId="720090138" templatename="剧情配音" voiceDes="林惊羽/别说丧气话。对了，不知上山这么久来，你修炼的怎么样了，干脆我们切磋切磋！" voiceId="5076"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300158" taskVoiceId="720090139" templatename="剧情配音" voiceDes="张小凡/我天资愚笨，与你是不能比的。不过我也很想见识一下你的修行。" voiceId="5077"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300159" taskVoiceId="720090140" templatename="剧情配音" voiceDes="张小凡/唔…..好痛呀！" voiceId="5078"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300160" taskVoiceId="720090141" templatename="剧情配音" voiceDes="林惊羽/小凡，小凡，你没事吧！我一时失手……" voiceId="5079"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300161" taskVoiceId="720090142" templatename="剧情配音" voiceDes="张小凡/没什么，我没事。" voiceId="5080"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300162" taskVoiceId="720090143" templatename="剧情配音" voiceDes="田灵儿/你凭什么欺负人？看绫！" voiceId="5081"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300163" taskVoiceId="720090144" templatename="剧情配音" voiceDes="齐昊/师弟！快陪个不是！" voiceId="5082"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300164" taskVoiceId="720090145" templatename="剧情配音" voiceDes="林惊羽/小凡，刚才是我不对，说是试一下各自修为，但出手没有分寸，对不起了。" voiceId="5083"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300165" taskVoiceId="720090146" templatename="剧情配音" voiceDes="张小凡/没、没什么，师父，是弟子的不好，一切都是弟子的错……" voiceId="5084"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300166" taskVoiceId="720090147" templatename="剧情配音" voiceDes="田不易/闭嘴，没用的东西！" voiceId="5085"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300167" taskVoiceId="720090148" templatename="剧情配音" voiceDes="林惊羽/矮胖子！你做什么！" voiceId="5086"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300168" taskVoiceId="720090149" templatename="剧情配音" voiceDes="齐昊/多谢田师叔手下留情！" voiceId="5087"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300169" taskVoiceId="720090150" templatename="剧情配音" voiceDes="田不易/哼，你们去吧！" voiceId="5088"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300170" taskVoiceId="720090151" templatename="剧情配音" voiceDes="苏茹/你们也下去吧，我和你们师父还有话说。" voiceId="5089"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300171" taskVoiceId="720090152" templatename="剧情配音" voiceDes="宋大仁/是！师娘！" voiceId="5090"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300172" taskVoiceId="720090153" templatename="剧情配音" voiceDes="苏茹/今天为何发这么大的火？" voiceId="5091"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300173" taskVoiceId="720090154" templatename="剧情配音" voiceDes="田不易/适才灵儿与林惊羽动手时，齐昊凝冰成墙挡住林惊羽的斩龙剑，你可看清楚了？" voiceId="5092"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300174" taskVoiceId="720090155" templatename="剧情配音" voiceDes="苏茹/哎，他并没有祭出寒冰剑就做到了，我大竹峰门下无一人是他的对手。" voiceId="5093"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300175" taskVoiceId="720090156" templatename="剧情配音" voiceDes="田不易/……老七怎么样了？" voiceId="5094"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300176" taskVoiceId="720090157" templatename="剧情配音" voiceDes="苏茹/还能怎么样，被你这位大仙人打得吐血了呗！" voiceId="5095"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300177" taskVoiceId="720090158" templatename="剧情配音" voiceDes="田不易/……你今晚去看看他，带上大黄丹，免得他明日装死。" voiceId="5096"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300178" taskVoiceId="720090159" templatename="剧情配音" voiceDes="张小凡/师娘，您怎么来了？" voiceId="5097"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300179" taskVoiceId="720090160" templatename="剧情配音" voiceDes="苏茹/我来看看你的伤，还好，没有什么大碍，服下这颗大黄丹吧。" voiceId="5098"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300180" taskVoiceId="720090161" templatename="剧情配音" voiceDes="张小凡/多谢师娘。" voiceId="5099"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300181" taskVoiceId="720090162" templatename="剧情配音" voiceDes="苏茹/是你师父叫我拿来的。你这个师父，性子要强，又死要面子，其实他动完手就后悔了，你可不要怨恨他呀。" voiceId="5100"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300182" taskVoiceId="720090163" templatename="剧情配音" voiceDes="张小凡/师娘，我不敢怪师父，我太笨了才惹得师父不高兴。" voiceId="5101"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300183" taskVoiceId="720090164" templatename="剧情配音" voiceDes="苏茹/唉，他烦心的不是这个。你师父修为虽高，大竹峰却后继无人，这么重的担子压在身上，他心里也是很苦的。" voiceId="5102"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300184" taskVoiceId="720090165" templatename="剧情配音" voiceDes="张小凡/师娘……我……" voiceId="5103"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300185" taskVoiceId="720090166" templatename="剧情配音" voiceDes="苏茹/你看我，我和你一个孩子说这些做什么，你早些歇息吧，我走了。" voiceId="5104"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300186" taskVoiceId="720090167" templatename="剧情配音" voiceDes="田灵儿/你站在那里发什么呆？还在怨恨我爹白天拿你出气？" voiceId="5105"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300187" taskVoiceId="720090168" templatename="剧情配音" voiceDes="张小凡/不是的，是我笨才惹师父生气的……" voiceId="5106"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300188" taskVoiceId="720090169" templatename="剧情配音" voiceDes="田灵儿/你呀，就这个性子，给，拿着这个！" voiceId="5107"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300189" taskVoiceId="720090170" templatename="剧情配音" voiceDes="张小凡/“太极玄清道”法决！师姐，你这……" voiceId="5108"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300190" taskVoiceId="720090171" templatename="剧情配音" voiceDes="田灵儿/哼，我爹一向看不起你，你拿着这份法决，偷偷修习，到时候修炼个名堂出来给他看看，别再像今日这么丢脸了。" voiceId="5109"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300191" taskVoiceId="720090172" templatename="剧情配音" voiceDes="张小凡/可是师姐，万一被知道了……" voiceId="5110"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300192" taskVoiceId="720090173" templatename="剧情配音" voiceDes="田灵儿/哼，最多骂我几句，关一段时日禁闭，那又怎么样？反正我可不能让你受人欺负！" voiceId="5111"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300193" taskVoiceId="720090174" templatename="剧情配音" voiceDes="张小凡/师姐……你真好……" voiceId="5112"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300194" taskVoiceId="720090175" templatename="剧情配音" voiceDes="田灵儿/什么人？！" voiceId="5113"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300195" taskVoiceId="720090176" templatename="剧情配音" voiceDes="张小凡/原来是你呀，师姐，快停手！" voiceId="5114"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300196" taskVoiceId="720090177" templatename="剧情配音" voiceDes="田灵儿/你这个做朋友要督促小凡勤加修炼，虽然再练也比不上齐昊师兄，但是至少要打得过那个什么臭屁林惊羽。" voiceId="5115"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300197" taskVoiceId="720090178" templatename="剧情配音" voiceDes="田灵儿/哼！不要乱说我齐昊大哥，不理你们了，我走了！" voiceId="5116"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300198" taskVoiceId="720090179" templatename="剧情配音" voiceDes="田不易/昨天的事，你们都看到了？" voiceId="5117"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300199" taskVoiceId="720090180" templatename="剧情配音" voiceDes="宋大仁/是，师父大展神威，出手惩戒那两个……" voiceId="5118"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300200" taskVoiceId="720090181" templatename="剧情配音" voiceDes="田不易/放屁！你们该看的是别脉的师兄弟修为深厚。不说齐昊，就连刚入门三年的小家伙也胜过你们大多数人，你们知不知道？" voiceId="5119"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300201" taskVoiceId="720090182" templatename="剧情配音" voiceDes="宋大仁/弟子惭愧……" voiceId="5120"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300202" taskVoiceId="720090183" templatename="剧情配音" voiceDes="田不易/七脉会武转眼即至，你们这些不成器的家伙，从今日起，全部闭关，不修出一个样子来，看我不剥了你们的皮！" voiceId="5121"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300203" taskVoiceId="720090184" templatename="剧情配音" voiceDes="张小凡/毕竟我负责大竹峰的伙食，我要是闭关了，谁来做饭呀。" voiceId="5122"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300204" taskVoiceId="720090185" templatename="剧情配音" voiceDes="张小凡/……哎呀，米粮又不够了，你能帮我去河阳采购一些吗？" voiceId="5123"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300205" taskVoiceId="720090186" templatename="剧情配音" voiceDes="谢百惑/这位少侠，想要点什么？" voiceId="5124"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300206" taskVoiceId="720090187" templatename="剧情配音" voiceDes="谢百惑/咳咳，充足倒是充足，只是……" voiceId="5125"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300207" taskVoiceId="720090188" templatename="剧情配音" voiceDes="谢百惑/少侠有所不知，河阳城的货物赋税都受控于黑龙帮。少侠要是需求量大，还须知会黑龙帮一声。" voiceId="5126"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300208" taskVoiceId="720090189" templatename="剧情配音" voiceDes="少帮主/哎呦！早知你要来河阳，我就去京城混了！少侠不要打了！你就看在我英俊潇洒、风流不羁的份上，再饶过我一次吧。" voiceId="5127"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300209" taskVoiceId="720090190" templatename="剧情配音" voiceDes="少帮主/看我唏嘘的眼神，帅气的胡茬子……" voiceId="5128"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300210" taskVoiceId="720090191" templatename="剧情配音" voiceDes="少帮主/我走我走……（连滚带爬的跑远了，忽然又回头大喊）我还会再回来的！" voiceId="5129"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300211" taskVoiceId="720090192" templatename="剧情配音" voiceDes="谢百惑/哎，你有所不知，这黑龙帮虽然被打退，但是背后有人撑腰，迟早会卷土重来的。" voiceId="5130"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300212" taskVoiceId="720090193" templatename="剧情配音" voiceDes="谢百惑/正是我们本城的城防，伍统领。" voiceId="5131"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300213" taskVoiceId="720090194" templatename="剧情配音" voiceDes="谢百惑/少侠真是侠义心肠，这次你负责的采购，我给你打折。" voiceId="5132"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300214" taskVoiceId="720090195" templatename="剧情配音" voiceDes="张小凡/怎么耽搁了这么久？" voiceId="5133"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300215" taskVoiceId="720090196" templatename="剧情配音" voiceDes="张小凡/我六师兄杜必书下山寻宝回来了。" voiceId="5134"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300216" taskVoiceId="720090197" templatename="剧情配音" voiceDes="张小凡/他做了三个骰子法宝，把师父气的晚饭都没吃呢。" voiceId="5135"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300217" taskVoiceId="720090198" templatename="剧情配音" voiceDes="杜必书/一年多没见，你没怎么变嘛。" voiceId="5136"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300218" taskVoiceId="720090199" templatename="剧情配音" voiceDes="杜必书/你……哎，我只是一时兴起创作的，何必这么世俗眼光呢。" voiceId="5137"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300219" taskVoiceId="720090200" templatename="剧情配音" voiceDes="杜必书/不说这个了……正好多日未见，你我切磋一下吧。" voiceId="5138"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300220" taskVoiceId="720090201" templatename="剧情配音" voiceDes="杜必书/不打了，不打了，本以为我修为大进，没想到还不是你的对手。" voiceId="5139"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300221" taskVoiceId="720090202" templatename="剧情配音" voiceDes="杜必书/你……我去找大师兄去，懒得理你。" voiceId="5140"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300222" taskVoiceId="720090203" templatename="剧情配音" voiceDes="张小凡/你知道吗？这届七脉会武每脉出九人，而我大竹峰门下加上我和师姐一共才八人，所以我也能参加呢！" voiceId="5141"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300223" taskVoiceId="720090204" templatename="剧情配音" voiceDes="张小凡/谢谢你这么肯定我，我……我要去做功课了，我一定会加油的。" voiceId="5142"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300224" taskVoiceId="720090205" templatename="剧情配音" voiceDes="曾书书/且慢动手，听我解释。" voiceId="5143"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300225" taskVoiceId="720090206" templatename="剧情配音" voiceDes="曾书书/阁下且停一下，听我一言，我可不是外人，我是风回峰弟子曾书书！" voiceId="5144"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300226" taskVoiceId="720090207" templatename="剧情配音" voiceDes="曾书书/嘿嘿，同为青云弟子，我来是为了和各脉师兄弟门拉近一下关系。" voiceId="5145"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300227" taskVoiceId="720090208" templatename="剧情配音" voiceDes="曾书书/你怎么这么看人！我实话实说吧，小弟生平最爱奇珍异兽，最近听闻张小凡师弟偶得一只灵猴，所以才悄悄的跟着阁下！" voiceId="5146"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300228" taskVoiceId="720090209" templatename="剧情配音" voiceDes="曾书书/嘿嘿，目的你知道了，不如你帮我个忙，帮我……" voiceId="5147"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300229" taskVoiceId="720090210" templatename="剧情配音" voiceDes="曾书书/……这样，半个月后，我带你去观看七脉会武，顺便再送你青云第一美女陆雪琪的丹青墨宝如何？" voiceId="5148"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300230" taskVoiceId="720090211" templatename="剧情配音" voiceDes="曾书书/嘿嘿，我想把小灰偷走养几天过过瘾，最多三日一定归还，怎么样？" voiceId="5149"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300231" taskVoiceId="720090212" templatename="剧情配音" voiceDes="小灰/吱吱！" voiceId="5150"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300232" taskVoiceId="720090213" templatename="剧情配音" voiceDes="曾书书/哎呀，破相了，这猴子跑了！" voiceId="5151"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300233" taskVoiceId="720090214" templatename="剧情配音" voiceDes="曾书书/算了，天生灵物自然各有其主，我好歹也算抱过三眼灵猴了，值了，哈哈哈！" voiceId="5152"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300234" taskVoiceId="720090215" templatename="剧情配音" voiceDes="曾书书/我说话算话，七天后就带你去七脉会武，看看这一甲子一次的盛会！" voiceId="5153"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300235" taskVoiceId="720090216" templatename="剧情配音" voiceDes="张小凡/你终于来了，我正四处找你呢。" voiceId="5154"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300236" taskVoiceId="720090217" templatename="剧情配音" voiceDes="张小凡/说起来好紧张！我今天下午要和风回峰的彭昌师兄比武呢……" voiceId="5155"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300237" taskVoiceId="720090218" templatename="剧情配音" voiceDes="彭昌/在下风回峰彭昌，请指教。" voiceId="5156"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300238" taskVoiceId="720090219" templatename="剧情配音" voiceDes="张小凡/我……赢了？可是，我刚才到底做了什么？" voiceId="5157"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300239" taskVoiceId="720090220" templatename="剧情配音" voiceDes="张小凡/最近总觉得情绪难以控制，你陪我出去走走吧。" voiceId="5158"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300240" taskVoiceId="720090221" templatename="剧情配音" voiceDes="田灵儿/这么晚了，你们在这里干什么？" voiceId="5159"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300241" taskVoiceId="720090222" templatename="剧情配音" voiceDes="田灵儿/不和你们说了，我要先走了！（脸红着跑开了。）" voiceId="5160"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300242" taskVoiceId="720090223" templatename="剧情配音" voiceDes="张小凡/我们回去吧……（经过刚才一幕，张小凡的心情似乎变得更加低落）" voiceId="5161"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300243" taskVoiceId="720090224" templatename="剧情配音" voiceDes="张小凡/我下一场的对手是朝阳峰的楚誉宏师兄。我要回去准备了！明天好好战一场！" voiceId="5162"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300244" taskVoiceId="720090225" templatename="剧情配音" voiceDes="楚誉宏/朝阳峰楚誉宏，请指教。" voiceId="5163"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300245" taskVoiceId="720090226" templatename="剧情配音" voiceDes="张小凡/我……赢了，我……赢……（张小凡缓缓走来，浑身衣衫尽数烧焦，有的地方还在冒着青烟，话未说完，人已经晕了过去）" voiceId="5164"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300246" taskVoiceId="720090227" templatename="剧情配音" voiceDes="张小凡/（缓缓醒来）呃……好痛……" voiceId="5165"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300247" taskVoiceId="720090228" templatename="剧情配音" voiceDes="张小凡/师父来看过我了？我伤势没好不方便行动，你能帮我去谢过师父吗？后山有些不错的茶叶，你去帮我采一些送给师父。" voiceId="5166"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300248" taskVoiceId="720090229" templatename="剧情配音" voiceDes="田不易/你来干什么？茶叶？这小子，不好好养伤，整这些没用的做什么" voiceId="5167"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300249" taskVoiceId="720090230" templatename="剧情配音" voiceDes="田不易/这个老七！正好，你既然来了，我倒想问问，你可知晓老七在比试中使用的第四层心法是谁教他的？" voiceId="5168"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300250" taskVoiceId="720090231" templatename="剧情配音" voiceDes="田不易/哼！算了，你就是不说我也能猜出几分，肯定是灵儿那丫头。罢了，告诉老七那小子，下场比试给我好好打！" voiceId="5169"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300251" taskVoiceId="720090232" templatename="剧情配音" voiceDes="张小凡/你回来啦，师父他还好嘛？" voiceId="5170"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300252" taskVoiceId="720090233" templatename="剧情配音" voiceDes="张小凡/什么意思？" voiceId="5171"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300253" taskVoiceId="720090234" templatename="剧情配音" voiceDes="张小凡/多谢你，吃了大黄丹后，果然感觉好多了，没想到我竟然会进入四强……" voiceId="5172"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300254" taskVoiceId="720090235" templatename="剧情配音" voiceDes="张小凡/别笑了，我现在很紧张，听说下一个对手是小竹峰的陆雪琪师姐，她可是个天才，我现在心里很没底气。" voiceId="5173"/>
