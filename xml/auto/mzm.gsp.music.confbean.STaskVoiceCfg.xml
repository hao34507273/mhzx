  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300072" taskVoiceId="720090077" templatename="主线配音" voiceDes="鬼王/乖徒儿回来了，此次外出历练可有收获？" voiceId="73"/>
  <mzm.gsp.music.confbean.STaskVoiceCfg id="720300073" taskVoiceId="720090078" templatename="主线配音" voiceDes="鬼王/修行如逆水行舟，不进则退，你能有此认识，为师深感欣慰。" voiceId="74"/>
